@page "/created-offers"

@inject ExchangeOrderService ExchangeOrderService
@inject UserService UserService

@if (UserService.IsAuthorized())
{
    <OffersListComponent IsRecieved="false" Offers="ExchangeOrderService.GetCreatedExchangeOrderOffers(UserService.CurrentUser.Email)"></OffersListComponent>
}
else
{
    <NotAuthorizedComponent></NotAuthorizedComponent>
}





@page "/created-orders"

@inject ExchangeOrderService ExchangeOrderService
@inject UserService UserService

@if (UserService.IsAuthorized())
{
    <OrderListComponent AllExchangeOrders="@ExchangeOrderService.GetCreatedExchangeOrders(UserService.CurrentUser.Email)" IsCreatedOrders="true"></OrderListComponent>
}
else
{
    <NotAuthorizedComponent></NotAuthorizedComponent>
}



@page "/select-start-order"

@inject ExchangeOrderService ExchangeOrderService
@inject UserService UserService

@if (UserService.IsAuthorized())
{
    <OrderListComponent AllExchangeOrders="@ExchangeOrderService.GetCreatedExchangeOrders(UserService.CurrentUser.Email)" IsTargetOrder="true"></OrderListComponent>
}
else
{
    <NotAuthorizedComponent></NotAuthorizedComponent>
}
